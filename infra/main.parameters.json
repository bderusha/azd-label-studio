{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "apiAppExists": {
      "value": "${SERVICE_LABEL_STUDIO_RESOURCE_EXISTS=false}"
    },
    "labelStudioUsername": {
      "value": "${LABEL_STUDIO_USERNAME=admin@localhost}"
    },
    "labelStudioPassword": {
      "value": "${LABEL_STUDIO_PASSWORD=password}"
    },
    "labelStudioDisableSignupWithoutLink": {
      "value": "${LABEL_STUDIO_DISABLE_SIGNUP_WITHOUT_LINK=true}"
    }
  }
}
